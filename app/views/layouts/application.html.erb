<!DOCTYPE html>
<html>
  <head>
    <title>W5D1App</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if logged_in? %>
      <%= current_user.username %>
      <%= button_to "Log Out", session_url , method: :delete %>
    <% end %>
    <%= yield %>
  </body>
</html>
